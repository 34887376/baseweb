<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byName" default-lazy-init="false">
	
	<!-- 用户登录 -->
	<bean id="iUserLoginDAO" class="com.ms.dao.user.face.impl.UserLoginDAOImpl"/>
	<!-- 查询促销 -->
	<bean id="iPromotionDAO" class="com.ms.dao.promotion.face.impl.PromotionDAOImpl"/>
	<!-- 阶梯规则 -->
	<bean id="iLadderDAO" class="com.ms.dao.ladder.face.impl.LadderDAOImpl"/>
	<!-- 商品信息 -->
	<bean id="iSkuDAO" class="com.ms.dao.sku.face.impl.SkuDAOImpl"/>
	<!-- 促销序列 -->
	<bean id="iPromotionSequenceDAO" class="com.ms.dao.promotionsequence.face.impl.PromotionSequenceDAOImpl"/>
	<!-- 阶梯促销关系 -->
	<bean id=iLadderPromotionDAO class="com.ms.dao.ladderpromotion.face.impl.LadderPromotionDAOImpl"/>
</beans>